# AI-Showmaker TypeScript Agent Configuration
# Copy this file to .env and fill in your actual values

# Inference.net Configuration
INFERENCE_NET_KEY=your_inference_net_api_key_here
INFERENCE_NET_BASE_URL=https://api.inference.net/v1
INFERENCE_NET_MODEL=meta-llama/llama-3.2-3b-instruct/fp-16

# AWS Remote Server Configuration
AWS_HOST=ec2-54-206-17-243.ap-southeast-2.compute.amazonaws.com
AWS_USER=ec2-user
AWS_KEY_PATH=/path/to/your/aws/key.pem

# Optional: Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json

# Optional: Agent Configuration
AGENT_TEMPERATURE=0.1
AGENT_MAX_TOKENS=4096
AGENT_TIMEOUT=30000
